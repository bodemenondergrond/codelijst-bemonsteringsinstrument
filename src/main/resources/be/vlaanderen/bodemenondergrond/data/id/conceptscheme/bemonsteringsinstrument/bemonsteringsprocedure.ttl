@prefix colt: <https://data.bodemenondergrond.vlaanderen.be/id/collection/bemonsteringsinstrument/> .
@prefix cs:   <https://data.bodemenondergrond.vlaanderen.be/id/conceptscheme/> .
@prefix ct:   <https://data.bodemenondergrond.vlaanderen.be/id/concept/bemonsteringsinstrument/> .
@prefix dc:   <http://purl.org/dc/elements/1.1/> .
@prefix dct:  <http://purl.org/dc/terms/> .
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xkos: <http://rdf-vocabulary.ddialliance.org/xkos#> .
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .

cs:bemonsteringsinstrument
        rdf:type            skos:ConceptScheme ;
        dc:identifier       "bemonsteringsinstrument" ;
        skos:definition     "Beschrijving welke procedure werd gevolgd bij de bemonstering."@nl ;
        skos:hasTopConcept  ct:bam , ct:boc ;
        skos:prefLabel      "Codelijst bemonsteringsinstrument."@nl .

ct:boc  rdf:type           skos:Concept ;
        dc:identifier      "boc" ;
        skos:inScheme      cs:bemonsteringsinstrument ;
        skos:prefLabel     "Compendium BOC"@nl ;
        skos:topConceptOf  cs:bemonsteringsinstrument .

ct:bam  rdf:type           skos:Concept ;
        dc:identifier      "bam" ;
        skos:inScheme      cs:bemonsteringsinstrument ;
        skos:prefLabel     "Compendium BAM"@nl ;
        skos:topConceptOf  cs:bemonsteringsinstrument .

colt:bemonsteringsinstrumenten
        rdf:type        skos:Collection ;
        dc:identifier   "bemonsteringsinstrumenten" ;
        skos:inScheme   cs:bemonsteringsinstrument ;
        skos:member     ct:bam , ct:boc ;
        skos:prefLabel  "Collectie van bemonsteringsinstrumenten."@nl .
